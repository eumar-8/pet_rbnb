<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url(<%= image_path("dogs-background.jpg") %>);">
  <div class="banner-content">

   <h1> ALL THE PETS LOVE YOU CAN HANDLE</h1>
    <div class="search-container">
      <%= form_tag("/search", class: "search-form") do %>
          <%= text_field_tag(:q, nil, placeholder: "Search for dog, cat or elephant")%>
          <a href="" class="search-button">
            <span><i class="fa fa-search"></i></span>
          </a>
       <% end %>
      </div>
      <p>
        <span id="banner-typed-text"></span>
      </p>

    </div>
  </div>
</div>
<img src='https://kitt.lewagon.com/placeholder/users/ssaunier' class="avatar-large"/>

<form action="">
  <div class="form-inputs">
    <input type="text" class="super-input">
  </div>
</form>

  <div class="form-actions">
    <a href="" class="btn btn-primary btn-lg">Submit</a>
  </div>

